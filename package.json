{
  "name": "ott-starter-kit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "cz",
    "dev": "APP_ENV=development next dev",
    "build": "APP_ENV=development next build",
    "start": "APP_ENV=development next start",
    "prepare": "husky install",
    "release": "standard-version",
    "lint": "eslint src"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.2",
    "@onrewind/ui": "^2.52.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-swipeable-views": "^0.13.2",
    "@types/swiper": "^6.0.0",
    "algoliasearch": "^4.13.1",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "cookies-next": "^2.0.4",
    "date-fns": "^2.28.0",
    "flowbite": "^1.7.0",
    "flowbite-react": "^0.5.0",
    "next": "12.1.6",
    "next-seo": "^5.4.0",
    "next-sitemap": "^3.0.5",
    "nextjs-progressbar": "^0.0.14",
    "react": "17",
    "react-dom": "17",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.14.2",
    "react-swipeable-views": "^0.14.0",
    "swiper": "^10.0.4",
    "tailwind-merge": "^1.2.1"
  },
  "devDependencies": {
    "@onrewind/eslint-config": "^3.3.0",
    "@origins-digital/types": "^8.29.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/tailwindcss": "^3.0.10",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "lint-staged": ">=10",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.57.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
